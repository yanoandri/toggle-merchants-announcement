{
    "manifest_version": 3,
    "name": "Remove Announcement",
    "version": "1.0",
    "description": "Remove Xendit announcement",
    "permissions": ["scripting", "activeTab", "storage"],
    "host_permissions": ["https://github.com/orgs/xendit/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}